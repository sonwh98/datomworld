(ns datomworld.main
  (:require [cljd.flutter :as f]
            ["package:flutter/material.dart" :as m]))

(defn main []
  (f/run
   (m/MaterialApp
    .title "Datomworld"
    .theme (m/ThemeData .primarySwatch m.Colors/blue)
    .home (m/Scaffold
           .appBar (m/AppBar .title (m/Text "Datomworld"))
           .body (m/Center
                  .child (m/Text "Hello Datomworld!"))))))